<% if flash[:alerts] %>
    <%= js add_gritter(flash[:alert], title: "Dylan Salay Portfolio", sticky: false) %>
<% elsif flash[:error] %>
    <%= js add_gritter(flash[:error], title: "Dylan Salay Portfolio", sticky: false) %>
<% else %>
    <%= js add_gritter(flash[:notice], title: "Dylan Salay Portfolio", sticky: false) %>
<% end %>